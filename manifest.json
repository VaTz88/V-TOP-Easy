{
    "manifest_version": 2,
    "name": "V-TOP Easy",
    "version": "0.1.0",
    "description": "Better experience with using VIT student login.",
    "author": "Vatsal Joshi",
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "V-TOP Easy"
    },
    "permissions": [
        "tabs"
    ],
    "content_scripts": [
